[flake8]
max-line-length = 120
extend-ignore = E203, W503
ignore =
    E501,
    W291, W292, W293, W391,
    E302, E303, E305,
    E231, E265, E117, E211,
    F401, F811, F841,
    E722
exclude =
    .git,
    __pycache__,
    venv,
    env,
    **/migrations/**,
    **/__pycache__/**
per-file-ignores =
    */migrations/*.py: E501,F401
